{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://trickbaseai.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.trickbaseai.com"
        }
      ]
    }
  ]
}




